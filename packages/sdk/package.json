{
  "name": "@worldfork/sdk",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/generated/schema.d.ts",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gen": "openapi-typescript ../../openapi/openapi.json -o src/generated/schema.d.ts",
    "build": "tsup src/index.ts --format esm --dts --clean",
    "smoke": "tsx scripts/smoke.ts"
  },
  "dependencies": {
    "openapi-fetch": "^0.10.0"
  },
  "devDependencies": {
    "openapi-typescript": "^7.5.0",
    "tsup": "^8.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  }
}
